    "web-vitals": "^2.1.4",
    "cypress": "^9.6.1",
    "xlsx": "^0.18.5",
    "jsonpath": "^1.1.1"
  },
  "scripts": {
    "start": "set PORT=3000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test && cypress run",
    "eject": "react-scripts eject",
    "runHeadless": "npx cypress run --headless",
